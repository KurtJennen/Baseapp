<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 1.3//EN" "http://struts.apache.org/dtds/tiles-config_1_3.dtd">
<tiles-definitions>
	<definition name="myTile" path="/jsp/tile.jsp">
		<put name="header" value="/jsp/header.jsp" />
		<put name="footer" value="/jsp/footer.jsp" />
		<put name="menu" value="/jsp/strutsmenu.jsp" />
		<put name="messages" value="/jsp/messages.jsp" />
	</definition>
	
	<definition name="display.error.form" extends="myTile">
		<put name="body" value="/jsp/error.jsp" />
		<put name="messages" value="blank.jsp" />
	</definition>
	
