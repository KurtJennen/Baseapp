	</definition>
	
	<definition name="display.body.form" extends="myTile">
		<put name="body" value="/jsp/body.jsp" />
	</definition>
	
	<definition name="display.detail.menu.form" extends="myTile">
		<put name="body" value="/jsp/security/displayDetailMenuForm.jsp" />
