<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-lazy="false">
	<class name="be.luxuryoverdosis.framework.data.to.BatchStepExecution" table="batch_step_execution">
		<id name="id" column="Step_Execution_Id" type="long" unsaved-value="-1" />
		<property name="version" column="Version" type="long" not-null="true"></property>
		<property name="stepName" column="Step_Name" type="string" not-null="true"></property>
		<property name="startTime" column="Start_Time" type="timestamp" not-null="true"></property>
		<property name="endTime" column="End_Time" type="timestamp"></property>
		<property name="status" column="Status" type="string"></property>
		<property name="commitCount" column="Commit_Count" type="long"></property>
		<property name="readCount" column="Read_Count" type="long"></property>
		<property name="filterCount" column="Filter_Count" type="long"></property>
		<property name="writeCount" column="Write_Count" type="long"></property>
		<property name="readSkipCount" column="Read_Skip_Count" type="long"></property>
		<property name="writeSkipCount" column="Write_Skip_Count" type="long"></property>
		<property name="processSkipCount" column="Process_Skip_Count" type="long"></property>
		<property name="rollbackCount" column="Rollback_Count" type="long"></property>
		<property name="exitCode" column="Exit_Code" type="string"></property>
		<property name="exitMessage" column="Exit_Message" type="string"></property>
		<property name="lastUpdated" column="Last_Updated" type="timestamp"></property>
		
		<many-to-one name="batchJobExecution" column="Job_Execution_Id" not-null="true" class="be.luxuryoverdosis.framework.data.to.BatchJobExecution" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>