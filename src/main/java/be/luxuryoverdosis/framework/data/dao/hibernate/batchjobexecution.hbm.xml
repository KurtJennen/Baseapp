<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-lazy="false">
	<class name="be.luxuryoverdosis.framework.data.to.BatchJobExecution" table="batch_job_execution">
		<id name="id" column="Job_Execution_Id" type="long" unsaved-value="-1" />
		<property name="version" column="Version" type="long"></property>
		<property name="createTime" column="Create_Time" type="timestamp" not-null="true"></property>
		<property name="startTime" column="Start_Time" type="timestamp"></property>
		<property name="endTime" column="End_Time" type="timestamp"></property>
		<property name="status" column="Status" type="string"></property>
		<property name="exitCode" column="Exit_Code" type="string"></property>
		<property name="exitMessage" column="Exit_Message" type="string"></property>
		<property name="lastUpdated" column="Last_Updated" type="timestamp"></property>
		
		<many-to-one name="batchJobInstance" column="Job_Instance_Id" not-null="true" class="be.luxuryoverdosis.framework.data.to.BatchJobInstance" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>